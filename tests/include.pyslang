include "itoa.libslang"

macro write 1 1 syscall3 drop end

macro ANSI_COLOR
	"\033[38;2;" write
	itoa write ";" write
	itoa write ";" write
	itoa write "m" write
end

macro ANSI_RESET
	"\033[0m"
end

0 while dup 255 < do
	0 while dup 255 < do
		dup2 255 ANSI_COLOR "0" write ANSI_RESET write
		16 +
	end drop
	"\n" write
	16 +
end drop

0 exit
