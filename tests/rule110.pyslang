macro SIZE 15 end
macro LOOP SIZE 2 - end
macro write
	1 1 syscall3 drop
end

mem LOOP + 1 .32

0 while dup LOOP < do
	0 while dup SIZE < do
		dup mem + ,32 :bool if
			"*" write
		else
			" " write
		end
		1 +
	end drop
	"\n" write
	mem     ,32 1 << mem 1 + ,32 |

	1 while dup LOOP < do
		swap 1 << 7 &
		over mem + 1 + ,32 |
		dup2 110 swap >> 1 &
		swap mem + swap .32
		swap
		1 +
	end drop drop
	1 +
end drop

0 exit
